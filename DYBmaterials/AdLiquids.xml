<!-- Properties change:
     In doc6709, it updates the GdLS, LS, MO properties that need be
     implemented in NuWa. 
     Here modify the density and element components of the liquids.
-->

<!-- Mineral Oil -->
<material	name="MineralOil" density="0.8511*g/cm3">
  <component	name="Carbon"	fractionmass="0.86475"/>
  <component	name="Hydrogen"	fractionmass="0.13525"/>
  <tabprops	address="/dd/Materials/OilProperties/OilAbsorptionLength"/>
  <tabprops	address="/dd/Materials/OilProperties/OilRefractionIndex"/>
  <tabprops	address="/dd/Materials/OilProperties/OilRAYLEIGH"/>
</material>

<!-- Liquid Scintillator -->
<material	name="LiquidScintillator"	density="0.8590*g/cm3">
  <!--component	name="Carbon"	fractionmass="0.87924"/-->
  <component	name="TS_C_of_Graphite"	fractionmass="0.87924"/>
  <!--component	name="Hydrogen"	fractionmass="0.1201"/-->
  <!--component	name="TS_H_of_Polyethylene"	fractionmass="0.1201"/-->
  <component	name="TS_H_of_Water"	fractionmass="0.1201"/>
  <component	name="Oxygen"	fractionmass="0.00034"/>
  <component	name="Nitrogen"	fractionmass="0.00027"/>
  <component	name="Sulfur"	fractionmass="0.00005"/>
  <tabprops	address="/dd/Materials/LSProperties/LSAbsorptionLength"/>
  <tabprops	address="/dd/Materials/LSProperties/LSRefractionIndex"/>
  <tabprops	address="/dd/Materials/LSProperties/LSFast"/>
  <tabprops	address="/dd/Materials/LSProperties/LSSlow"/>
  <tabprops	address="/dd/Materials/LSProperties/LSReemission"/>
  <tabprops	address="/dd/Materials/LSProperties/LSRAYLEIGH"/>
  <tabprops	address="/dd/Materials/LSProperties/LSScintillationYield"/>
  <tabprops	address="/dd/Materials/LSProperties/LSResolutionScale"/>
  <tabprops	address="/dd/Materials/LSProperties/LSFastTimeConstant"/>
  <tabprops	address="/dd/Materials/LSProperties/LSSlowTimeConstant"/>
  <tabprops	address="/dd/Materials/LSProperties/LSYieldRatio"/>
  <tabprops	address="/dd/Materials/LSProperties/GammaLSFastTimeConstant"/>
  <tabprops	address="/dd/Materials/LSProperties/GammaLSSlowTimeConstant"/>
  <tabprops	address="/dd/Materials/LSProperties/GammaLSYieldRatio"/>
  <tabprops	address="/dd/Materials/LSProperties/NeutronLSFastTimeConstant"/>
  <tabprops	address="/dd/Materials/LSProperties/NeutronLSSlowTimeConstant"/>
  <tabprops	address="/dd/Materials/LSProperties/NeutronLSYieldRatio"/>
  <tabprops	address="/dd/Materials/LSProperties/AlphaLSFastTimeConstant"/>
  <tabprops	address="/dd/Materials/LSProperties/AlphaLSSlowTimeConstant"/>
  <tabprops	address="/dd/Materials/LSProperties/AlphaLSYieldRatio"/>
</material>

<!-- Gd Doped LS 
     In doc6615-v2, the measured concentrations of Gd in AD1 is 0.1031% and in AD2 is 0.1032%,
     so take the average for the gadolinium concentration in simulation.
-->
<material	name="GdDopedLS"	density="0.8602*g/cm3">
  <!--component	name="Carbon"		fractionmass="0.87705"/-->
  <component	name="TS_C_of_Graphite"		fractionmass="0.87705"/>
  <!--component	name="Hydrogen"		fractionmass="0.12051"/-->
  <!--component	name="TS_H_of_Polyethylene"	fractionmass="0.12051"/-->
  <component	name="TS_H_of_Water"	fractionmass="0.12051"/>
  <component	name="Oxygen"		fractionmass="0.00109"/>
  <component	name="Gadolinium"	fractionmass="0.0010315"/>
  <component	name="Nitrogen"		fractionmass="0.00027"/>
  <component	name="Sulfur"	fractionmass="0.00005"/>
  <tabprops	address="/dd/Materials/GdLSProperties/GdLSAbsorptionLength"/>
  <tabprops	address="/dd/Materials/GdLSProperties/GdLSRefractionIndex"/>
  <tabprops	address="/dd/Materials/GdLSProperties/GdLSFast"/>
  <tabprops	address="/dd/Materials/GdLSProperties/GdLSSlow"/>
  <tabprops	address="/dd/Materials/GdLSProperties/GdLSReemission"/>
  <tabprops	address="/dd/Materials/GdLSProperties/GdLSRAYLEIGH"/>
  <tabprops	address="/dd/Materials/GdLSProperties/GdLSScintillationYield"/>
  <tabprops	address="/dd/Materials/GdLSProperties/GdLSResolutionScale"/>
  <tabprops	address="/dd/Materials/GdLSProperties/GdLSFastTimeConstant"/>
  <tabprops	address="/dd/Materials/GdLSProperties/GdLSSlowTimeConstant"/>
  <tabprops	address="/dd/Materials/GdLSProperties/GdLSYieldRatio"/>
  <tabprops	address="/dd/Materials/GdLSProperties/GammaGdLSFastTimeConstant"/>
  <tabprops	address="/dd/Materials/GdLSProperties/GammaGdLSSlowTimeConstant"/>
  <tabprops	address="/dd/Materials/GdLSProperties/GammaGdLSYieldRatio"/>
  <tabprops	address="/dd/Materials/GdLSProperties/NeutronGdLSFastTimeConstant"/>
  <tabprops	address="/dd/Materials/GdLSProperties/NeutronGdLSSlowTimeConstant"/>
  <tabprops	address="/dd/Materials/GdLSProperties/NeutronGdLSYieldRatio"/>
  <tabprops	address="/dd/Materials/GdLSProperties/AlphaGdLSFastTimeConstant"/>
  <tabprops	address="/dd/Materials/GdLSProperties/AlphaGdLSSlowTimeConstant"/>
  <tabprops	address="/dd/Materials/GdLSProperties/AlphaGdLSYieldRatio"/>
</material>

